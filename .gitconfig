[user]
	name = Lisa Ugray
	email = lisa.ugray@shopify.com
[merge]
	tool = vimdiff
	conflictstyle = diff3
[alias]
	st = status
	co = commit
	ch = checkout
	alias = config --get-regexp ^alias\\.
	unstage = reset HEAD --
	remove = reset HEAD --
	gl = log --oneline --graph --all
	permission-reset = !git diff -p -R | grep -E \"^(diff|(old|new) mode)\" | git apply
	graphical = !(gitk --all &> /dev/null &)
	k = !(gitk --all &> /dev/null &)
	fpt = force-push-this
	m = checkout master
	amco = commit -a --amend --no-edit
	prune-all-hard = !git fetch --all --prune && git branch -vv --no-color | grep \"\\[.*: gone\\]\" | awk \"{print \\$1}\" | xargs git branch -D
	checkoutr = checkout
	br = branch
	rtt = reset-to-tracking
[push]
	default = simple
[color "diff"]
	whitespace = red reverse
	commit = green
	frag = cyan
	old = red
	new = green
	meta = yellow
	func = bold magenta
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[diff "plist"]
	textconv = plutil -convert xml1 -o -
[color]
	ui = auto
[core]
	excludesfile = /Users/lisaugray/.global_gitignore
	pager = /usr/local/bin/diff-highlight | less
[pull]
	ff = only
[track-push]
	default = lugray
[fetch]
	prune = true
[diff]
	algorithm = patience
[protocol]
	version = 2
[credential]
	helper = osxkeychain
[url "https://github.com/Shopify/"]
	insteadOf = git@github.com:Shopify/
	insteadOf = git@github.com:shopify/
	insteadOf = ssh://git@github.com/Shopify/
	insteadOf = ssh://git@github.com/shopify/
